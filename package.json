{
  "name": "mura-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "start:local": "npm run dev | node ssl-proxy.js",
    "build": "cp mura.config.json public/mura.config.json && next build",
    "start": "next start",
    "test": "eslint src && echo \"no test specified\"",
    "lint": "eslint src",
    "lint-fix": "eslint src --fix",
    "export": "next build && next export",
    "help": "next export --help",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "update:mura": "npm uninstall mura.js @murasoftware/next-core @murasoftware/next-modules-bs4 @murasoftware/next-core-assets && npm install mura.js @murasoftware/next-core@^2.0.0 @murasoftware/next-modules-bs4 @murasoftware/next-core-assets"
  },
  "husky": {
    "hooks": {}
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@murasoftware/next-core": "^2.0.15",
    "@murasoftware/next-core-assets": "^1.0.1",
    "@murasoftware/next-modules-bs4": "^1.0.112",
    "bootstrap": "^4.6.0",
    "cross-env": "^7.0.3",
    "eslint-config-next": "^13.1.6",
    "fs": "^0.0.1-security",
    "hastscript": "^6.0.0",
    "hosted-git-info": "^2.8.9",
