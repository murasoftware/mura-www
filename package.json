    "mura.js": "^1.0.402",
    "next": "^13.1.6",
    "next-http-proxy-middleware": "^1.2.4",
    "next-transpile-modules": "^8.0.0",
    "node-fetch": "^3.2.10",
    "npm": "^8.13.1",
    "react": "^18.2.0",
    "react-bootstrap": "^1.5.2",
    "react-dom": "^18.2.0",
    "react-fontawesome": "^1.7.1",
    "react-loader-spinner": "^5.3.4",
    "react-markdown": "^5.0.3",
    "react-slick": "^0.29.0",
    "rehype-format": "^3.1.0",
    "remark-directive": "^1.0.1",
    "remark-gfm": "^1.0.0",
    "remark-rehype": "^8.1.0",
